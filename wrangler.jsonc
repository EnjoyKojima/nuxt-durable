{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "nuxt-cron",
  "main": "./.output/server/index.mjs",
  "compatibility_date": "2025-01-29",
  "assets": {
    "binding": "ASSETS",
    "directory": "./.output/public/"
  },
  "observability": {
    "enabled": true
  },

  "triggers": {
    "crons": [
      "* * * * *"
    ]
  }
}
